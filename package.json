{
  "name": "frame-throttle",
  "version": "1.1.0",
  "description": "A lightweight way to throttle events using requestAnimationFrame",
  "main": "dist/frame-throttle.js",
  "typings": "dist/frame-throttle.d.ts",
  "scripts": {
    "compile": "tsc",
    "pretest": "npm run compile",
    "prepublish": "npm test",
    "test": "tape dist/tests/*.tests.js",
    "test-cov": "npm run pretest && istanbul cover tape -- dist/tests/*.tests.js",
    "coveralls": "npm run test-cov -- --report lcovonly && cat coverage/lcov.info | coveralls"
  },
  "keywords": [
    "events",
    "frame",
    "handler",
    "listener",
    "requestAnimationFrame",
    "throttle"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/pelotoncycle/frame-throttle.git"
  },
  "author": "Mike Deverell <mike@devrelm.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/pelotoncycle/frame-throttle/issues"
  },
  "homepage": "https://github.com/pelotoncycle/frame-throttle#readme",
  "devDependencies": {
    "coveralls": "^2.11.12",
    "istanbul": "0.4.5",
    "jsdom": "^9.4.1",
    "mock-raf": "0.0.2",
    "sinon": "^1.17.5",
    "tape": "^4.6.0",
    "tape-catch": "^1.0.6",
    "typescript": "^1.8.10"
  }
}
